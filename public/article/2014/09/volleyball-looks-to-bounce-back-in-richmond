				   var navHeight = $( window ).height() - 500;
						 if ($(window).scrollTop() > navHeight) {
							 $('nav').addClass('fixed');
						 }
						 else {
							 $('nav').removeClass('fixed');
						 }
					});
				});
			</script>
			<nav class="share-elements">
				<ul class="share-boxes unstyled inline">
